<div class="page-header">
  <h1><%= t 'notification.p' %></h1>
</div>

<% @notifications.each do |notification| %>
  <div class="well">
    <%= link_to notification do %>
      <%= image_tag notification.image_path %>
      <%= raw notification.content %>
    <% end %>
  </div>
<% end %>

<%= will_paginate @notifications %>