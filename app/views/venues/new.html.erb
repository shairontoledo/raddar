<h1>New venue</h1>

<%= simple_form_for(@venue, :html => {:class => 'form-horizontal'}) do |f| %>
  <%= f.error_notification %>

  <div id="map_new_venue" class="map_canvas" ></div>

  <%= render 'venues/form_inputs', {form: f} %>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>


<%= link_to 'Back', venues_path %>
